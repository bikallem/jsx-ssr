{
  "name": "jsx-ssr",
  "version": "0.0.0",
  "description": "PPX experiments in ocaml/reasonml.",
  "esy": { "build": "refmterr dune build -p #{self.name} -j4" },
  "scripts": {
    "fmt": "esy dune build @fmt --auto-promote",
    "test": "esy dune runtest --auto-promote",
    "clean": "esy dune clean",
    "all": "esy dune build @all"
  },
  "dependencies": {
    "reason": "facebook/reason#master",
    "@opam/fmt": "0.8.6",
    "@opam/ocaml-migrate-parsetree": "*",
    "@opam/ppx_tools_versioned": "5.2.2",
    "@opam/uutf": "1.0.2",
    "@reason-native/console": "*",
    "@reason-native/rely": "*",
    "ocaml": "4.7.1"
  },
  "devDependencies": {
    "@opam/merlin": "*",
    "@opam/ocamlformat": "*",
    "@opam/rtop": "*",
    "@opam/utop": "*",
    "refmterr": "*",
    "@opam/dune": "*"
  }
}
